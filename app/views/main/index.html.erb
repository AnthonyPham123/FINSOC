
<div class="PageIndex">

    

    <div id="finsoc-banner">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
              </ol>
          <div class="carousel-inner" role="listbox">
           <div class="item active carousel-img PageIndex-carousel-img">
              <div class="PageIndex-carousel-text">
                <h1>
                  Finance & Banking Society
                </h1>
                <h2>
                  University of New South Wales<br/>
                  FINSOC
                </h2>
              </div>
              <% if  show_new_page %>

                  <a href="https://docs.google.com/forms/d/e/1FAIpQLSeKHEb1Ah3dmet1YujrW6woqu164Y44sUk57kTRFfPMk3RR_Q/viewform" target="_blank"><div class="event-box">
                      <!--<b>UPCOMING EVENTS</b>-->
                      <b class="darkButton">2018 SIGN UP &rarr; </b>
                      </div></a>
              <% end %>
             <%= image_tag "events/default-min-2.jpg", id:"finsoc-banner-logo", alt: "UNSW FINSOC" %>
           </div>
              <div class="item carousel-img PageIndex-carousel-img">
              <div class="PageIndex-carousel-text">
                <h1>
                  Read our latest blog!
                </h1>
                <h2>
                  A Quantamental analysis primer<br/>
                  
                </h2>
              </div>
              <% if  show_new_page %>

                  <a href="http://www.unswfinsoc.org.au/blog" target="_blank"><div class="event-box">
                      <!--<b>UPCOMING EVENTS</b>-->
                      <b>View Here </b>
                      </div></a>
              <% end %>
             <%= image_tag "forest.jpg", id:"finsoc-banner-logo", alt: "UNSW FINSOC" %>
           </div>
              <div class="item carousel-img PageIndex-carousel-img">
              <div class="PageIndex-carousel-text">
                <h1>
                  Get involved with FINSOC!
                </h1>
                <h2>
                  Join our <a href="http://eepurl.com/bQ0Pmv" target="_blank"><b>Mailing List </b></a> and follow us on <a href="https://www.facebook.com/FINSOCUNSW/" target="_blank"><b>Facebook </b></a><br/>
                  
                </h2>
              </div>
              <% if  show_new_page %>

                  <a href="https://docs.google.com/forms/d/e/1FAIpQLSeKHEb1Ah3dmet1YujrW6woqu164Y44sUk57kTRFfPMk3RR_Q/viewform" target="_blank"><div class="event-box">
                      <!--<b>UPCOMING EVENTS</b>-->
                      <b>Sign up for FINSOC &rarr; </b>
                      </div></a>
              <% end %>
             <%= image_tag "team.png", id:"finsoc-banner-logo", alt: "UNSW FINSOC" %>
           </div>
          </div>
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="right carousel-control" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
              </a>
        </div>
    </div>

    <div class="PageIndex-featured container">
      <%= link_to course_guide_path, :class=> "PageIndex-courseGuideFeature", method: :get do %>
            <div class="PageIndex-courseGuideFeature-text">
              <p>View our</p>
              <p>Finance Course Guide</p>
             <span class="glyphicon glyphicon-info-sign PageIndex-courseGuideFeature-icon" aria-hidden="true"></span> 
            </div>
        <% end %>
        <div class="Event">
            <%= link_to events_path, method: :get do %>

            <div class="Event-imgContainer">
                  <div class="Event-imgContainer-link">
                      <%= image_tag "events/2018/resumeWorkshop.jpg", :class=> "Event-imgContainer-img Event-imgContainer-img--showRight", alt: "Resume workshop" %>
                  </div>
            </div>
            <% end %>
            <div class="Event-title">
            Resume workshop

            </div>
        </div>
        <div class="Event Event-past">
            <%= link_to events_path, method: :get do %>

            <div class="Event-imgContainer">
                    <%= image_tag "events/2018/family.jpg", :class=> "Event-imgContainer-img", alt: "Family Feud" %>
            </div>
            <% end %>
            <div class="Event-title">
            Family Feud
            </div>
        </div>
        <div class="Event Event-past">
            <%= link_to events_path, method: :get do %>

            <div class="Event-imgContainer">
                    <%= image_tag "events/2018/intern.jpg", :class=> "Event-imgContainer-img", alt: "O-week" %>
            </div>
            <% end %>
            <div class="Event-title">
            Intern panel
            </div>
        </div>
        
        
    </div>
    <div class="section2">
      <div class="section-text container" style="padding-bottom:0px;">
        <div class="Card PageIndex-missionCard">
            <div class="title">
                <h2>Our Mission</h2>
            </div>
            <div class="PageIndex-missionCard-text">
                <p>To become Australia&#8217;s leading student run society platform in assisting students on achieving their banking and financial planning career goals.</p>
            </div>
        </div>
      </div>
    </div>
